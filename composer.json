{
    "name": "miguelgarces/consulta-contraloria",
    "description": "Paquete para consultas en la contraloria",
    "type": "library",
    "keywords": ["Contraloria", "Laravel", "Scrapping"],
    "require": {
        "php": ">=7",
        "fabpot/goutte": "^4.0",
        "illuminate/support": "^5.5|^6|^7|^8",
        "spatie/pdf-to-text": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5|^7.5|^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "MiguelGarces\\ConsultaContraloria\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MiguelGarces\\ConsultaContraloria\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MiguelGarces\\ConsultaContraloria\\ContraloriaServiceProvider"
            ],
            "aliases": {
                "ConsultaContraloriaNit": "MiguelGarces\\ConsultaContraloria\\Facades\\ConsultaContraloriaNitFacade"
            }
        }
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Miguel Garces",
            "email": "miguelgarces2415@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
